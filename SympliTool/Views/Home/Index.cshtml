@model string

<div class="panel panel-success">
    <div class="panel-heading text-center"><h4>Hi, you will be searching @Model</h4></div>
    <div class="panel-body">
        <form class="p-a-1" asp-action="Search" method="get">
            <div class="form-group">
                <label>Keywords:</label>
                <input class="form-control" name="keywords" placeholder="Enter keywords to search" />
            </div>
            <div class="form-group">
                <label>URL:</label>
                <input class="form-control" name="url" placeholder="Enter an URL" />
            </div>
            <button class="btn btn-primary" type="submit">Search</button>
        </form>
    </div>
</div>


